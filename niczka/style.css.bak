body {
	background-color: #a2836e;
}
p.headlane {
	font-family: Florence, cursive;
	font-size: 50px;
}
.topnav {
    overflow: hidden;
    background-color: #674d3c;
}
.topnav a {
    float: left;
    color: #FFF;
    text-align: center;
    padding: 14px 20px;
    text-decoration: none;
    font-size: 20px;
}
.topnav a:hover {
	background-color: #d9ad7c;
}
.topnav a.active {
	background-color: #fff2df;
	color: black;
}